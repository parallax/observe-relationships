{
  "name": "parallax/algolia-relationships",
  "type": "library",
  "description": "Algolia Relationships",
  "keywords": ["laravel","scout","algolia","search","relationship","eloquent"],
  "homepage": "https://parall.ax",
  "license": "MIT",
  "require": {
    "laravel/framework": "^5.5 | 6.* | 7.*",
    "laravel/scout": "^5.0",
    "algolia/algoliasearch-client-php": "^1.27"
  },
  "archive": {
    "exclude": [
      "/tests",
      "tests",
      "tests/*.*",
      "phpunit.xml",
      ".gitignore",
      ".gitattributes",
      "README.md"
    ]
  },
  "autoload": {
    "psr-4": {
      "Parallax\\AlgoliaRelationship\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Parallax\\AlgoliaRelationship\\AlgoliaRelationshipProvider"
      ]
    }
  }
}
